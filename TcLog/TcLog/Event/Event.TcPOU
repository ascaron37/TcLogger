      </Set>
    </Property>
    <Method Name="raise_alarm" Id="{b92c7d97-5a9f-4a9c-8999-52ff932d2446}">
      <Declaration><![CDATA[METHOD PRIVATE raise_alarm
VAR_INPUT
  level: EventLevel;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[logging.raise_alarm(
  id:=id,
  path:=path,
  message:=message,
  level:=level);]]></ST>
      </Implementation>
    </Method>
    <Method Name="set_id" Id="{ca8e16af-abb6-4e45-b03a-925bd73ad678}">
