    <Method Name="alarm" Id="{4734d13d-7054-4601-88d0-576b7693b39a}">
      <Declaration><![CDATA[METHOD PUBLIC alarm
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
