    <Method Name="submit" Id="{5cbfa2c1-1c09-4567-9983-e434249344bc}">
      <Declaration><![CDATA[METHOD PRIVATE submit
VAR_INPUT
  level: EventLevel;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[logging.parameter := parameter;
logging.submit_event(
  id:=id,
  path:=path,
  message:=_message,
  level:=level,
  cleared:=level <> EventLevel.ALARM);]]></ST>
      </Implementation>
