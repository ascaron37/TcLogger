    </Method>
    <Method Name="alarm_if_true" Id="{ed06e891-83e8-4023-b03a-28331a2191c2}">
      <Declaration><![CDATA[METHOD alarm_if_true
VAR_INPUT
    value: BOOL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF value THEN
  alarm();
ELSE
  clear_alarm();
END_IF]]></ST>
      </Implementation>
    </Method>
