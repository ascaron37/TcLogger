      </Implementation>
    </Method>
    <Method Name="info_if_true" Id="{83861da7-e1af-4da2-959b-ff5ff145a653}">
      <Declaration><![CDATA[METHOD info_if_true
VAR_INPUT
    value: BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF value THEN
  info();
END_IF]]></ST>
      </Implementation>
    </Method>
