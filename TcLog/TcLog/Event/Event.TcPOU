      <Declaration><![CDATA[PROPERTY PUBLIC instance_path : STRING(255)]]></Declaration>
      <Get Name="Get" Id="{502f1fc3-998f-4e3e-ac0b-443913534009}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
