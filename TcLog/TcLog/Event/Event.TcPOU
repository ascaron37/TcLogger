        </Implementation>
      </Get>
      <Set Name="Set" Id="{c6c5bffc-0b16-4216-8776-007889c8b72b}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[_logging REF= logging;
set_id(logging.generate_event_id());]]></ST>
        </Implementation>
      </Set>
    </Property>
    <Property Name="message" Id="{95b2f84d-eb7a-4759-a78c-209d6dcdc019}">
