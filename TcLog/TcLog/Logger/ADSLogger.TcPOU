<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.9">
  <POU Name="ADSLogger" Id="{f353a23b-59c1-4eb7-b1b3-77f5986ff606}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK ADSLogger IMPLEMENTS EventLogger
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
  _level: EventLevel;
  _disable_ads_output: BOOL;
  _mask: DWORD;
  _prefix: STRING(15);
  _formatted_output: STRING(255);
  _path: STRING(255);
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="ads_log" Id="{1bc6e817-4323-49bf-a63f-a94d3b6c886f}">
      <Declaration><![CDATA[METHOD PRIVATE ads_log
VAR_IN_OUT
  ev: EventMessage;
END_VAR
VAR
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[mask := create_mask(ev.level);
prefix := create_prefix(ev.level);
path := create_path(ev.path);

formatted_output := CONCAT(CONCAT(ev.path, prefix), ev.message);

IF NOT disable_ads_output THEN
  ADSLOGSTR(mask, formatted_output, '');
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="create_mask" Id="{aee6a054-3e4f-421c-aca5-2c735f9fea76}">
      <Declaration><![CDATA[METHOD PRIVATE create_mask : DWORD
VAR_IN_OUT
  level: EventLevel;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE level OF
EventLevel.INFO:
  create_mask := ADSLOG_MSGTYPE_HINT;
EventLevel.WARNING:
  create_mask := ADSLOG_MSGTYPE_WARN;
EventLevel.ERROR, EventLevel.ALARM:
  create_mask := ADSLOG_MSGTYPE_ERROR;
ELSE
  create_mask := ADSLOG_MSGTYPE_HINT;
END_CASE]]></ST>
      </Implementation>
    </Method>
    <Method Name="create_path" Id="{59a6e2af-a873-437a-9a9c-126ed2450a1d}">
      <Declaration><![CDATA[METHOD PRIVATE create_path : STRING(255)
VAR_IN_OUT
  path: STRING(255);
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[create_path := CONCAT(' | ', path);]]></ST>
      </Implementation>
    </Method>
    <Method Name="create_prefix" Id="{e6393b63-a206-438e-accf-a721f61f3d0a}">
      <Declaration><![CDATA[METHOD PRIVATE create_prefix : STRING(15)
VAR_INPUT
  level: EventLevel;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE level OF
EventLevel.INFO:
  create_prefix := ' | Info: ';
EventLevel.WARNING:
  create_prefix := ' | Warning: ';
EventLevel.ERROR:
  create_prefix := ' | Error: ';
EventLevel.Alarm:
  create_prefix := ' | Alarm: ';
ELSE
  create_prefix := ' | Undefined: ';
END_CASE]]></ST>
      </Implementation>
    </Method>
