    <Method Name="submit_event_message" Id="{72fa51cd-5f83-427b-a2c1-e007bce7d269}">
      <Declaration><![CDATA[METHOD PUBLIC submit_event_message
VAR_IN_OUT
	ev: EventMessage;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF ev.level >= level THEN
  ads_log(ev);
END_IF]]></ST>
      </Implementation>
    </Method>
  </POU>
